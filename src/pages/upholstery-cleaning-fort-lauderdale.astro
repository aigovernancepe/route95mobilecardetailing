  "description": description,
  "url": `${siteUrl}/upholstery-cleaning-fort-lauderdale/`,
  "serviceType": "Mobile Upholstery Cleaning",
  "provider": { "@id": `${siteUrl}/#business` },
  "areaServed": [
    { "@type": "City", "name": "Fort Lauderdale" },
    { "@type": "City", "name": "Dania Beach" },
